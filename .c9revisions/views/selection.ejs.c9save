{"ts":1365907841828,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\n<html>\n  <head>\n    <title>Election Data: <%=item.name%></title>\n    <link rel=\"stylesheet\" href=\"/style.css\">\n  </head>\n  <body>\n    <a href=\"/\"><h1>Election Data</h1></a>\n    <dl>\n      <dt>Party name</dt>\n      <dd><%=item.name%></dd>\n      <% if (item.description) { %>\n      <dt>Description</dt>\n      <dd><%=item.description%></dd>\n      <% } %>\n      <dt>Candidates</dt>\n      \n      <dd>\n        <% if (related_items.length > 0) { %>\n        <ul>\n          <% related_items.forEach(function(related_item) { %>\n          <li>\n            <a href=\"/candidates/<%=related_item._id%>\">\n              <%=related_item.name%>\n            </a>\n          </li>\n          <% }); %>\n        </ul>\n        <% } else { %>\n        No candidates have been added to this party.\n        <% } %>\n      </dd>\n    </dl>\n    <div class=\"control\">\n      <h2>Add a new candidate</h2>\n      <form id=\"create-candidate\" method=\"post\" action=\"/candidates/\">\n        <input type=\"hidden\" name=\"item[party]\" value=\"<%=item._id%>\">\n        <!-- The line above links the candidate to be created to this party. -->\n        <label>Candidate name: \n          <input name=\"item[name]\" type=\"text\" required\n                   placeholder=\"Ron Paul\">\n        </label>\n        <label>Candidate image URL:\n          <input name=\"item[image]\" type=\"url\" size=\"80\"\n                   placeholder=\"http://www.ronpaul2012.com/wp-content/themes/ronpaul/images/bg-header.jpg\">\n        </label>\n        <button>Add</button>\n      </form>\n    </div>\n  </body>\n</html>\n"]],"start1":0,"start2":0,"length1":0,"length2":1546}]],"length":1546}
